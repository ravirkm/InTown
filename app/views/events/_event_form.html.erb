<%= form_for @event do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
      <%= f.label :date %><br />
      <%= f.text_field :date %>
    <div id="datepicker">
      <%= f.label :address %><br />
      <%= f.text_field :address %>
    </div>
  <div class="actions">
    <%= f.submit "Submit" %>
  </div>
<% end %>

